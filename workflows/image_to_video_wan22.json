{
  "_meta": {
    "title": "Image to Video - Wan2.2 I2V (GGUF)",
    "description": "Animates a static image into video using Wan2.2 I2V 14B GGUF model. Higher resolution (1024x576) and faster than Wan2.1.",
    "version": "1.0",
    "updated": "2026-02-01",
    "requires": [
      "ComfyUI-GGUF",
      "ComfyUI-VideoHelperSuite",
      "Wan2.2-I2V-A14B Q5_K_M GGUF",
      "umt5-xxl-encoder Q5_K_M GGUF",
      "CLIP-ViT-H vision encoder",
      "wan_2.1_vae"
    ],
    "vram_required": "~14GB peak",
    "generation_time": "~7-15 min for 81 frames @ 30 steps on RTX 5060 Ti",
    "notes": "Uses GGUF quantized models for lower VRAM. Place diffusion model in models/unet/, text encoder in models/clip/. Resolution can go up to 1024x576."
  },
  "1": {
    "inputs": {
      "unet_name": "wan2.1-i2v-14b-480p-Q5_K_M.gguf"
    },
    "class_type": "UnetLoaderGGUF",
    "_meta": { "title": "Load Wan2.2 I2V GGUF Model" }
  },
  "2": {
    "inputs": {
      "vae_name": "wan_2.1_vae.safetensors"
    },
    "class_type": "VAELoader",
    "_meta": { "title": "Load VAE" }
  },
  "3": {
    "inputs": {
      "clip_name": "umt5-xxl-encoder-Q5_K_M.gguf",
      "type": "wan"
    },
    "class_type": "CLIPLoaderGGUF",
    "_meta": { "title": "Load CLIP Text Encoder (GGUF)" }
  },
  "4": {
    "inputs": {
      "clip_name": "CLIP-ViT-H-14-laion2B-s32B-b79K.safetensors"
    },
    "class_type": "CLIPVisionLoader",
    "_meta": { "title": "Load CLIP Vision" }
  },
  "5": {
    "inputs": {
      "image": "INPUT_IMAGE.png",
      "upload": "image"
    },
    "class_type": "LoadImage",
    "_meta": { "title": "Load Source Image" }
  },
  "6": {
    "inputs": {
      "text": "Realistic 4K cinematic video of a luxury artisan candle with a gently flickering warm flame. The flame dances softly with natural randomness, casting warm golden light across the wax surface. Soft ambient shadows shift slowly on the background wall. Shallow depth of field with the candle in sharp focus. Subtle warm light rays emanate from the flame. The camera holds steady with an extremely slow, barely perceptible dolly-in. Warm color temperature, cozy atmosphere, no rapid movement, photorealistic quality.",
      "clip": ["3", 0]
    },
    "class_type": "CLIPTextEncode",
    "_meta": { "title": "Positive Prompt (Motion)" }
  },
  "7": {
    "inputs": {
      "text": "static, frozen, no movement, blurry, jittery, distorted, morphing, warped hands, warped fingers, glitching, flicker, fast motion, unnatural movement, frame skipping, ghosting, double exposure, low quality, watermark, text overlay, compressed artifacts",
      "clip": ["3", 0]
    },
    "class_type": "CLIPTextEncode",
    "_meta": { "title": "Negative Prompt" }
  },
  "8": {
    "inputs": {
      "image": ["5", 0],
      "clip_vision": ["4", 0],
      "crop": "center"
    },
    "class_type": "CLIPVisionEncode",
    "_meta": { "title": "Encode Source Image" }
  },
  "9": {
    "inputs": {
      "positive": ["6", 0],
      "negative": ["7", 0],
      "vae": ["2", 0],
      "width": 1024,
      "height": 576,
      "length": 81,
      "batch_size": 1,
      "start_image": ["5", 0],
      "clip_vision_output": ["8", 0]
    },
    "class_type": "WanImageToVideo",
    "_meta": { "title": "Wan I2V Conditioning" }
  },
  "10": {
    "inputs": {
      "seed": 42,
      "steps": 30,
      "cfg": 3.5,
      "sampler_name": "euler_ancestral",
      "scheduler": "simple",
      "denoise": 1.0,
      "model": ["1", 0],
      "positive": ["9", 0],
      "negative": ["9", 1],
      "latent_image": ["9", 2]
    },
    "class_type": "KSampler",
    "_meta": { "title": "Sample Video" }
  },
  "11": {
    "inputs": {
      "samples": ["10", 0],
      "vae": ["2", 0]
    },
    "class_type": "VAEDecode",
    "_meta": { "title": "Decode Video" }
  },
  "12": {
    "inputs": {
      "frame_rate": 24,
      "loop_count": 0,
      "filename_prefix": "i2v_wan22",
      "format": "video/h264-mp4",
      "pingpong": false,
      "save_output": true,
      "images": ["11", 0]
    },
    "class_type": "VHS_VideoCombine",
    "_meta": { "title": "Save Video (MP4)" }
  }
}
